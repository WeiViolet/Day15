#include<stdio.h>
main()
{
	int i,n,m,j,k;
	printf("Size of array A and B: ");
	scanf("%d%d",&n,&m);
	int a[n],b[m],c[n+m];
	printf("Array A elements: ");
	for(i=0;i<n;i++)
	{		
		scanf("%d",&a[i]);
		c[i]=a[i];
	}
	k=i;
	printf("Array B elements: ");
	for(j=0;j<m;j++)
	{
		scanf("%d",&b[j]);
		c[k]=b[j];
		k++;
	}
	printf("Resultant array elements: ");
	for(i=0;i<k;i++)
	{
		printf("%d ",c[i]);
	}
}
