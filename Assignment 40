 #include<stdio.h>
  main()
  {
  	int i,age,Childage=0,Schoolage=0,Adultage=0;
  	for(i=1;i<=20;i++)
  	{
  		printf("Age= ",i);
	scanf("%d",&age);
  	if(age>=1 && age<=5)
  	{	Childage++;	 }
  	else if(age>=6 && age<=18)
  	{	Schoolage++;	}
  	else if(age>18)
  	{	Adultage++;    }
	}
	printf("Child age=%d\nSchool age=%d\nAdult age=%d",Childage,Schoolage,Adultage);
  }
